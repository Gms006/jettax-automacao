      "passo_a_passo": [
        {
          "passo": 1,
          "acao": "Definir Escopo",
          "logica_local": "Determine o período (competência) e os clientes a serem auditados."
        },
        {
          "passo": 2,
          "acao": "Solicitar Relatório",
          "endpoint": "POST https://api.jettax360.com.br/api/v1/fiscal/nfses/conference-report",
          "instrucao_ia": "Payload: { 'competence': 'MM/YYYY', 'clients': [array de IDs] }"
        },
        {
          "passo": 3,
          "acao": "Monitorar Status",
          "endpoint": "GET https://api.jettax360.com.br/api/v1/fiscal/nfses/conference-report",
          "instrucao_ia": "Faça polling até o status ser 'Concluído'. O campo 'status' indica o progresso."
        },
        {
          "passo": 4,
          "acao": "Baixar Planilha",
          "instrucao_ia": "Extraia a URL do relatório e baixe o arquivo Excel."
        }
      ]
    },
    {
      "id": "PROC-SERV-REL-002",
      "nome": "Verificar Status do EFD-Reinf",
      "objetivo": "Confirmar se a obrigação foi transmitida corretamente para a Receita Federal.",
      "passo_a_passo": [
        {
          "passo": 1,
          "acao": "Consultar Dados do Reinf",
          "endpoint": "GET https://api.jettax360.com.br/api/v1/fiscal/efdreinf",
          "instrucao_ia": "Verifique o campo 'status' da resposta. Status possíveis: 'Pendente', 'Transmitido', 'Erro'."
        },
        {
          "passo": 2,
          "acao": "Analisar Pendências (Se Aplicável)",
          "instrucao_ia": "Se houver erro, extraia o campo 'errorMessage' para diagnóstico."
        }
      ]
    },
    {
      "id": "PROC-SERV-REL-003",
