  "procedimentos": [
    {
      "id": "PROC-FED-REL-001",
      "nome": "Auditar Substituição Tributária",
      "objetivo": "Verificar se o ICMS-ST foi corretamente calculado nas notas de entrada.",
      "passo_a_passo": [
        {
          "passo": 1,
          "acao": "Selecionar Clientes e Período",
          "logica_local": "Defina a competência (MM/YYYY) e os IDs dos clientes."
        },
        {
          "passo": 2,
          "acao": "Solicitar Auditoria",
          "endpoint": "POST https://api-federal.jettax360.com.br/api/v1/commerce/audit-reprocess/",
          "instrucao_ia": "Payload: { 'type': '1', 'clients': [ids], 'competence': '10/2025' }"
        },
        {
          "passo": 3,
          "acao": "Aguardar Processamento",
          "endpoint": "GET https://api-federal.jettax360.com.br/api/v1/commerce/audit-reprocess/",
          "instrucao_ia": "Faça polling até 'status.label' == 'Finalizado'."
        },
        {
          "passo": 4,
          "acao": "Consultar Detalhes",
          "endpoint": "GET https://api-federal.jettax360.com.br/api/v1/commerce/audit-reprocess-details/{id}",
          "instrucao_ia": "Verifique os clientes processados e eventuais erros."
        }
      ]
    },
    {
      "id": "PROC-FED-REL-002",
      "nome": "Identificar Notas Faltantes",
      "objetivo": "Detectar saltos na numeração de NF-e/NFCe.",
      "passo_a_passo": [
        {
          "passo": 1,
          "acao": "Solicitar Análise",
          "endpoint": "POST https://api-federal.jettax360.com.br/api/v1/commerce/missing-invoices",
          "instrucao_ia": "Payload: { 'competence': '10/2025', 'clients': [ids] }"
        },
        {
          "passo": 2,
          "acao": "Consultar Resultado",
          "endpoint": "GET https://api-federal.jettax360.com.br/api/v1/commerce/missing-invoices/{id}/details",
          "instrucao_ia": "Liste os clientes e séries com notas faltantes."
        },
        {
          "passo": 3,
          "acao": "Baixar CSV",
          "endpoint": "GET https://api-federal.jettax360.com.br/api/v1/commerce/missing-invoices/{id}/download-csv",
          "instrucao_ia": "Exporte a lista completa em CSV."
        }
      ]
    },
    {
      "id": "PROC-FED-REL-003",
      "nome": "Gerar Relatório de Auditoria ICMS",
      "objetivo": "Consolidar resultado das auditorias de ICMS/ST em planilha.",
      "passo_a_passo": [
        {
          "passo": 1,
          "acao": "Solicitar Geração",
          "endpoint": "POST https://api-federal.jettax360.com.br/api/v1/commerce/hub/report/automation-icms",
          "instrucao_ia": "Payload: { 'competence': '10/2025', 'clients': [ids], 'type': 5 }"
